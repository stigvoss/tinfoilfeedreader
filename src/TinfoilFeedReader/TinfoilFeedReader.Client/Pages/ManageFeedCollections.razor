@page "/feedcollections/{FeedCollectionId:guid}/manage"
@inherits TinfoilFeedReader.Client.Pages.CodeBehinds.ManageFeedCollectionsPage


<SideBar>
    <FeedMenu FeedCollectionId=@FeedCollectionId />
</SideBar>

<main>
    <h1>Manage Collection</h1>

    @if (Collection is object)
    {
        foreach (var feed in Collection.Feeds.OrderBy(feed => feed.SequenceIndex))
        {
            <ManageFeed Feed=@feed />
        }
    }
</main>