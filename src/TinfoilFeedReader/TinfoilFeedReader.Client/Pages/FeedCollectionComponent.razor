@page "/feedcollection/{FeedCollectionId}"
@page "/feedcollection/{FeedCollectionId}/feed/{FeedId}"
@inherits TinfoilFeedReader.Client.Pages.CodeBehinds.FeedCollectionComponent

<h1>@Title</h1>

@if (Entries is object)
{
    foreach (var entry in Entries)
    {
        <p><a href="@entry.Link">@entry.Title</a> - <em>@entry.PublishDate.LocalDateTime</em></p>
    }
}